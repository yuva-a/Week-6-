CHIP main
{
    IN x[16],y[16];
    OUT out[16],OF;

    PARTS:
    
    //sign
    Xor(a=x[0],b=y[0],out=out[0]);

//Product of matissa
    multiplayer(x[0]=x[9],x[1]=x[10],x[2]=x[11],x[3]=x[12],x[4]=x[13],x[5]=x[14],x[6]=x[15],x[7]=true,y[0]=y[9],y[1]=y[10],y[2]=y[11],y[3]=y[12],y[4]=y[13],y[5]=y[14],y[6]=y[15],y[7]=ture,out[0]=mantiout0,out[1]=mantiout1,out[2]=mantiout2,out[3]=mantiout3,out[4]=mantiout4,out[5]=mantiout5,out[6]=mantiout6,out[7]=mantiout7,out[8]=mantiout8,out[9]=mantiout9,out[10]=mantiout10,out[11]=mantiout11,out[12]=mantiout12,out[13]=mantiout13,out[14]=mantiout14,out[15]=mantiout15);
    And(a=mantiout15,b=mantiout15,out=opp);
    Not(in=opp,out=nopp);

// normalizing or 2 to 1 mux in this case
    And(a=mantiout15,b=opp,out=1manti0);
