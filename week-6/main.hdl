//

//Overflow
    And(a=s[8],b=s[7],out=over0);
    Not(in=s[8],out=ns8);
    Not(in=s[7],out=ns7);
    And(a=ns7,b=ns8,out=over1);
    Or(a=over0,b=over1,out=over2);
    Or(a=over2,b=sf[8],out=over3);
