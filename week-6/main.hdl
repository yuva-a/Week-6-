    Or(a=1manti0,b=2manti0,out=out[9]);
    
    And(a=mantiout14,b=opp,out=1manti1);
    And(a=mantiout13,b=nopp,out=2manti1);
    Or(a=1manti1,b=2manti1,out=out[10]);
    
    And(a=mantiout13,b=opp,out=1manti2);
    And(a=mantiout12,b=nopp,out=2manti2);
    Or(a=1manti2,b=2manti2,out=out[11]);
    
    And(a=mantiout12,b=opp,out=1manti3);
    And(a=mantiout11,b=nopp,out=2manti3);
    Or(a=1manti3,b=2manti3,out=out[12]);
    
    And(a=mantiout11,b=opp,out=1manti4);
    And(a=mantiout10,b=nopp,out=2manti4);
    Or(a=1manti4,b=2manti4,out=out[13]);
    
    And(a=mantiout10,b=opp,out=1manti5);
    And(a=mantiout9,b=nopp,out=2manti5);
